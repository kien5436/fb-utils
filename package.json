    "web-ext-lint": "web-ext lint -s build"
  },
  "devDependencies": {
    "@babel/core": "^7.11.0",
    "@babel/preset-env": "^7.11.0",
    "babel-loader": "^8.1.0",
    "clean-webpack-plugin": "^3.0.0",
    "copy-webpack-plugin": "^6.0.1",
    "css-loader": "^3.5.3",
    "file-loader": "^6.0.0",
    "mini-css-extract-plugin": "^0.9.0",
    "node-sass": "^4.14.1",
    "optimize-css-assets-webpack-plugin": "^5.0.3",
    "pug-html-loader": "^1.1.5",
    "sass-loader": "^8.0.2",
    "terser-webpack-plugin": "^3.0.1",
    "web-ext-types": "^3.2.1",
    "webpack": "^4.43.0",
    "webpack-cli": "^3.3.11"
  },
  "dependencies": {
    "bulma": "^0.8.2",
    "content-scripts-register-polyfill": "^1.0.5",
    "lodash": "^4.17.20",
