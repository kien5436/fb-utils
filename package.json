{
  "name": "fb-utils",
  "version": "1.0.0",
  "main": "index.js",
  "license": "MIT",
  "scripts": {
    "build": "webpack",
    "pack-src": "zip -r -FS source.zip src INSTRUCTIONS.md package.json webpack.config.js yarn.lock",
    "release": "yarn web-ext-build && yarn pack-src",
    "release-chrome": "google-chrome --pack-extension=./build --pack-extension-key=./build.pem --disable-setuid-sandbox",
    "web-ext-build": "web-ext build -s build -a . -o",
    "web-ext-dev": "web-ext run -s build -p=dev",
    "web-ext-lint": "web-ext lint -s build"
  },
  "devDependencies": {
    "@babel/core": "^7.11.0",
    "@babel/plugin-transform-react-jsx": "^7.10.4",
    "@babel/plugin-transform-runtime": "^7.11.5",
    "@babel/preset-env": "^7.11.0",
    "babel-loader": "^8.1.0",
    "clean-webpack-plugin": "^3.0.0",
    "copy-webpack-plugin": "^6.0.1",
    "css-loader": "^3.5.3",
    "eslint": "^7.21.0",
    "eslint-config-pk": "^1.0.2",
    "eslint-config-preact": "^1.1.3",
    "file-loader": "^6.0.0",
    "html-webpack-plugin": "^4.5.0",
    "mini-css-extract-plugin": "^0.9.0",
    "optimize-css-assets-webpack-plugin": "^5.0.3",
    "pug-html-loader": "^1.1.5",
    "purgecss-webpack-plugin": "^4.0.2",
    "sass": "^1.32.8",
    "sass-loader": "^8.0.2",
    "terser-webpack-plugin": "^3.0.1",
    "web-ext-types": "^3.2.1",
    "webpack": "4",
    "webpack-cli": "^3.3.11"
  },
  "dependencies": {
    "@babel/runtime": "^7.11.2",
    "@babel/runtime-corejs3": "^7.11.2",
    "bulma": "^0.9.1",
    "content-scripts-register-polyfill": "^1.0.5",
    "lodash": "^4.17.20",
    "preact": "^10.5.4",
    "web-ext": "^5.2.0",
    "webextension-polyfill": "^0.6.0"
  }
}